{
  "name": "my-profile-page",
  "description": "profile page",
  "author": "Haouari Haitam kouider",
  "private": true,
  "scripts": {
    "build": "npm run build.types && npm run build.client && npm run build.ssg",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "build.ssg": "vite build --ssr src/entry.static.tsx && node ./server/entry.static.js",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "ssg": "node server/entry.static",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.9.0",
    "@builder.io/qwik-city": "0.0.111",
    "@emotion/react": "11.9.3",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "11.9.3",
    "@mui/material": "5.8.6",
    "@types/eslint": "8.4.6",
    "@types/node": "^20.3.2",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "autoprefixer": "^10.4.14",
    "eslint": "8.23.1",
    "eslint-plugin-qwik": "0.9.0",
    "node-fetch": "3.2.10",
    "postcss": "^8.4.24",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "4.8.3",
    "vite": "3.1.1",
    "vite-tsconfig-paths": "3.5.0",
    "wrangler": "0.0.0-kickoff-demo"
  },
  "dependencies": {
    "daisyui": "^2.52.0"
  }
}
